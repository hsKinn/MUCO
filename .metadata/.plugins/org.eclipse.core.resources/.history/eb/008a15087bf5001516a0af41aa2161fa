<script type="text/javascript">
	$(document).ready(function() {

		// 마우스 떼면 다시 원래 색으로
		$("#hitTheRoadMenu").mouseleave(function() {
			$("#hitTheRoadMenu").css("color", "#959595");
		});
		$("#myPackMenu").mouseleave(function() {
			$("#myPackMenu").css("color", "#959595");
		});
		$("#sharePackMenu").mouseleave(function() {
			$("#sharePackMenu").css("color", "#959595");
		});
		$("#recommendPlaceMenu").mouseleave(function() {
			$("#recommendPlaceMenu").css("color", "#959595");
		});
		$("#tripReservationMenu").mouseleave(function() {
			$("#tripReservationMenu").css("color", "#959595");
		});
		$("#personalInfoMenu").mouseleave(function() {
			$("#personalInfoMenu").css("color", "#959595");
		});

	});
</script>

