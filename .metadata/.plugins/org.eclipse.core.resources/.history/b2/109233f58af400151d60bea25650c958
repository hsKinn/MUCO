<?xml version="1.0" encoding="UTF-8"?>
<query>

	<country>
		<getAllCountryList>
			SELECT	COUNTRY_ID
					, COUNTRY_NAME
					, COUNTRY_COLOR
					, COUNTRY_FONT_COLOR
			FROM	COUNTRY
		</getAllCountryList>
		<getCountryInfoByCountryName>
			SELECT	COUNTRY_ID
					, COUNTRY_NAME
					, COUNTRY_COLOR
					, COUNTRY_FONT_COLOR
			FROM	COUNTRY
			WHERE	COUNTRY_NAME = ?
		</getCountryInfoByCountryName>
	</country>
	
	<evaluation>
	</evaluation>
	
