<script>
	jQuery.noConflict();
	jQuery(function() {
		
		var $ = jQuery;

		$('#focus-multiple').click(function() {
			$('#map1').vectorMap('set', 'focus', {
				regions : [ 'KR', 'JP' ],
				animate : true
			});
		});

		$('#focus-init').click(function() {
			$('#map1').vectorMap('set', 'focus', {
				scale : 1,
				x : 0.5,
				y : 0.5,
				animate : true
			});
		});

		$('#map1').vectorMap({
			map : 'world_mill_en',
