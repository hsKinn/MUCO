<script type="text/javascript">

	$(document).ready(function(){
		function toggle_visibility() {
		       var e = document.getElementById("menu1");
		       if(e.style.display == 'block')
		          e.style.display = 'none';
		       else
		          e.style.display = 'block';
		    }
	});

</script>
